{
	"@context": "http://schema.org",
	"@type": "ReviewAction",
	"target": {
		"@type": "EntryPoint",
		"urlTemplate": "https://localhost:1337/api/v1/review",
		"encodingType": "application/ld+json",
		"contentType": "application/ld+json"
	},
	"object" : {
		"@type": "Movie",
		"url-input": "required"
	},
	"result": {
		"@type": "Review",
		"url-output": "required",
		"reviewBody-input": "required",
		"reviewRating": {
			"ratingValue-input": "required"
		}
	}
}
