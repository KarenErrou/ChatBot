<#tt0120458-mr_ghost>
	schema:about <#tt0120458>;
	schema:author "mr_ghost";
	schema:dateCreated "Sat Jan 23 1999 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 6;
	schema:headline " AWESOME SCARY MOVIE!";
