	schema:about <#tt0126170>;
	schema:author "Siloty";
	schema:dateCreated "Tue Jun 17 2008 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 10;
	schema:headline " best Disney cartoon";
