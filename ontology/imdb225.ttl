<#tt0415679-atinder>
	schema:about <#tt0415679>;
	schema:author "atinder";
	schema:dateCreated "Mon Oct 08 2012 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 7;
	schema:headline " Nice fun little zombie movie.";
