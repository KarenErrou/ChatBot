<#tt0313487-BatmanFunReviews2018>
	schema:about <#tt0313487>;
	schema:author "BatmanFunReviews2018";
	schema:dateCreated "Sat Nov 10 2018 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 8;
	schema:headline " This movie has 13 on Rotten Tomatoes really?";
