<#tt0428870-ghajiniblue>
	schema:about <#tt0428870>;
	schema:author "ghajiniblue";
	schema:dateCreated "Thu Sep 10 2009 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 10;
	schema:headline " A Moment to Remember Rocks";
