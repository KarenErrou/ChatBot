<#tt0058203-demodokos>
	schema:about <#tt0058203>;
	schema:author "demodokos";
	schema:dateCreated "Wed May 03 2000 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 10;
	schema:headline " Funny and entertainingfor ages 7100";
