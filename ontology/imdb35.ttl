<#tt0463392-valdez_10>
	schema:about <#tt0463392>;
	schema:author "valdez_10";
	schema:dateCreated "Mon Jan 14 2008 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:headline " Plain terrible.";
