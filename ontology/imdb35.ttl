	schema:about <#tt0110475>;
	schema:author "Yale_6";
	schema:dateCreated "Tue Sep 02 2003 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 10;
	schema:headline " Cute but one disappointment";
