<#tt0832318-tmoefoto>
	schema:about <#tt0832318>;
	schema:author "tmoefoto";
	schema:dateCreated "Thu Aug 20 2009 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 4;
	schema:headline " wonderful acting but...";
