<#tt0101356-Martin_Jarborg>
	schema:about <#tt0101356>;
	schema:author "Martin_Jarborg";
	schema:dateCreated "Sun Mar 07 1999 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 6;
	schema:headline " Not the best plot but great lines.";
