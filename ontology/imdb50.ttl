<#tt0050086-ma_cortes>
	schema:about <#tt0050086>;
	schema:author "ma_cortes";
	schema:dateCreated "Wed Dec 01 2010 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 8;
	schema:headline " Famed and classic Western by Delmer Daves with a magnificent Glenn Ford and a restrained Van Heflin";
