<#tt1886493-jim9tan>
	schema:about <#tt1886493>;
	schema:author "jim9tan";
	schema:dateCreated "Fri Jan 17 2014 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 7;
	schema:headline " Check out the hot chicks";
