	schema:author "Exxon_1985";
	schema:dateCreated "Sat Apr 14 2007 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 9;
	schema:headline " Nothing really specialBoring Fast Food.";
