<#tt0346054-PingpongRueppel>
	schema:about <#tt0346054>;
	schema:author "PingpongRueppel";
	schema:dateCreated "Fri Apr 16 2004 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 6;
	schema:headline " Just bad";
