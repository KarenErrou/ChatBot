<#tt0109040-piajartist>
	schema:about <#tt0109040>;
	schema:author "piajartist";
	schema:dateCreated "Thu Jan 02 2003 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:headline " Not Jim's brightest hour at all.";
