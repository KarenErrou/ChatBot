<#tt0098966-Jezebel_Bartley>
	schema:about <#tt0098966>;
	schema:author "Jezebel_Bartley";
	schema:dateCreated "Fri May 03 2002 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 8;
