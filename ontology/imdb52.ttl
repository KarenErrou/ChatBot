<#tt1204773-talullah82>
	schema:about <#tt1204773>;
	schema:author "talullah82";
	schema:dateCreated "Thu May 09 2013 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 4;
	schema:headline " not a directing and acting perspective but congratulations and thank you";
