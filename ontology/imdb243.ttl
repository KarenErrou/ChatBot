<#tt0087034-toddhauser>
	schema:about <#tt0087034>;
	schema:author "toddhauser";
	schema:dateCreated "Sun Aug 17 2003 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 9;
	schema:headline " Makes you appreciate Opera!";
