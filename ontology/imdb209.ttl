<#tt1441940-ajit2106>
	schema:about <#tt1441940>;
	schema:author "ajit2106";
	schema:dateCreated "Tue Jul 30 2013 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 8;
	schema:headline " Very Moving and Poignant from the eyes of a 11 year old";
