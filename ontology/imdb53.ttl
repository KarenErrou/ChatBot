<#tt0114916-michellelocke007>
	schema:about <#tt0114916>;
	schema:author "michellelocke007";
	schema:dateCreated "Wed Jan 29 2003 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 10;
	schema:headline " wonderful and beautifully shot film";
