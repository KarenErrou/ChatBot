<#tt0482629-cbennett3491>
	schema:about <#tt0482629>;
	schema:author "cbennett3491";
	schema:dateCreated "Sat Mar 10 2007 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 10;
	schema:headline " It was very inspirational!";
