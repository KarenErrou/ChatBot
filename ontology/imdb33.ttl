<#tt0093996-dx4lifexpac>
	schema:about <#tt0093996>;
	schema:author "dx4lifexpac";
	schema:dateCreated "Sun Feb 18 2001 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:headline " Elvis goes psycho";
