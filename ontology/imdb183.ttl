	schema:about <#tt0090563>;
	schema:author "paul_sweeney";
	schema:dateCreated "Thu Jan 02 2003 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 10;
	schema:headline " A European Masterpiece";
