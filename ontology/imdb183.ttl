<#tt0092857-f_gimenez>
	schema:about <#tt0092857>;
	schema:author "f_gimenez";
	schema:dateCreated "Sat Sep 09 2000 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 6;
	schema:headline " The best Charles Bronson...";
