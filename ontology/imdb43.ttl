	schema:about <#tt0115710>;
	schema:author "jerry_dean67";
	schema:dateCreated "Fri Mar 17 2000 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 4;
	schema:headline " An enjoyable 7 rated film.";
