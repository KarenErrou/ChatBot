<#tt0082640-videorama_759_859391>
	schema:about <#tt0082640>;
	schema:author "videorama_759_859391";
	schema:dateCreated "Mon Jul 18 2016 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:headline " Not a real good lover film";
