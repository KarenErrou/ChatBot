<#tt0101540-ma_cortes>
	schema:about <#tt0101540>;
	schema:author "ma_cortes";
	schema:dateCreated "Sun Mar 04 2012 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 8;
	schema:headline " Released excon blames an advocate for his prison and schemes an insidious vendetta on his family";
