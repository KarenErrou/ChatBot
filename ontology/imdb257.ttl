<#tt0144608-BlackFlames>
	schema:about <#tt0144608>;
	schema:author "BlackFlames";
	schema:dateCreated "Fri Jul 12 2002 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 1;
