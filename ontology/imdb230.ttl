	schema:about <#tt0067992>;
	schema:author "rossrobinson";
	schema:dateCreated "Wed Nov 05 2003 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 8;
	schema:headline " fantastic";
