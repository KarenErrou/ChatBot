<#tt0095662-steve_216>
	schema:about <#tt0095662>;
	schema:author "steve_216";
	schema:dateCreated "Thu Nov 05 1998 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 10;
	schema:headline " LOTS OF LAUGHS";
