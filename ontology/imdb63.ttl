<#tt1621045-djblues_945_138858>
	schema:about <#tt1621045>;
	schema:author "djblues_945_138858";
	schema:dateCreated "Tue Oct 07 2014 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 2;
	schema:headline " Just. So. Bad.";
