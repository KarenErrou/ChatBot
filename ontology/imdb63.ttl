<#tt1316037-ernstdirk>
	schema:about <#tt1316037>;
	schema:author "ernstdirk";
	schema:dateCreated "Thu Dec 06 2012 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:headline " How did no one prevent this abomination from coming into being?";
