	schema:about <#tt0092086>;
	schema:author "jerbeargood";
	schema:dateCreated "Sun Dec 29 2002 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 7;
	schema:headline " Classic";
