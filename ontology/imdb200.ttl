<#tt0073629-Gutza>
	schema:about <#tt0073629>;
	schema:author "Gutza";
	schema:dateCreated "Fri Apr 07 2006 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 8;
	schema:headline " The...";
