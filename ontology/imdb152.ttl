<#tt0374345-jurgendemaeyer>
	schema:about <#tt0374345>;
	schema:author "jurgendemaeyer";
	schema:dateCreated "Mon Oct 20 2003 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 9;
	schema:headline " Best Belgian movie in years...";
