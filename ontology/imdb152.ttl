<#tt0417976-greenlantern74>
	schema:about <#tt0417976>;
	schema:author "greenlantern74";
	schema:dateCreated "Sun Dec 18 2005 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 1;
	schema:headline " crap!";
