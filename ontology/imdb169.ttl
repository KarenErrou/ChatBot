<#tt0062709-erichyoung>
	schema:about <#tt0062709>;
	schema:author "erichyoung";
	schema:dateCreated "Tue Sep 25 2001 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 8;
	schema:headline " A Spaghetti Westworld Comedy? Charming!";
