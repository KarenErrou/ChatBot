<#tt0085211-niz>
	schema:about <#tt0085211>;
	schema:author "niz";
	schema:dateCreated "Wed Jan 20 1999 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 10;
	schema:headline " Masterful kungfu hilarious slapstick a Jackie Chan beater";
