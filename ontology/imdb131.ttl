<#tt0115988-danielll_rs>
	schema:about <#tt0115988>;
	schema:author "danielll_rs";
	schema:dateCreated "Sat Oct 09 1999 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 7;
	schema:headline " Impressing and welldone";
