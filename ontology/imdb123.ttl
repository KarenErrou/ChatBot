<#tt0120353-kenshiro666>
	schema:about <#tt0120353>;
	schema:author "kenshiro666";
	schema:dateCreated "Sat Oct 24 2009 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 1;
