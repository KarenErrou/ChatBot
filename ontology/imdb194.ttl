	schema:about <#tt1311067>;
	schema:author "IssaGuy";
	schema:dateCreated "Sun Feb 18 2018 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 1;
	schema:headline " Worst Halloween movie ever made!";
