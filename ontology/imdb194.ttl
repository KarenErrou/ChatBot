	schema:about <#tt1311067>;
	schema:author "randyfromscream";
	schema:dateCreated "Sat Sep 16 2017 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 1;
	schema:headline " Made HalloweeN Resurrection look like a masterpiece";
