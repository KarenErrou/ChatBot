<#tt0058981-Carrie_the_Oracle>
	schema:about <#tt0058981>;
	schema:author "Carrie_the_Oracle";
	schema:dateCreated "Sun Oct 30 2005 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 3;
	schema:headline " Staged but same conclusion.";
