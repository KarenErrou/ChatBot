<#tt0203119-chimpacabra>
	schema:about <#tt0203119>;
	schema:author "chimpacabra";
	schema:dateCreated "Sat Aug 02 2003 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 10;
	schema:headline " one of my favorite";
