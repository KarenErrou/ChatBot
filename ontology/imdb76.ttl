<#tt0131857-Mickey_Knox>
	schema:about <#tt0131857>;
	schema:author "Mickey_Knox";
	schema:dateCreated "Thu Jan 25 2001 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 10;
	schema:headline " and i laughed and i laughed...";
