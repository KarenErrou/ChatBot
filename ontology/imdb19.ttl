	schema:about <#tt1196141>;
	schema:author "michaelhirakida";
	schema:dateCreated "Sat Aug 17 2013 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 4;
	schema:headline " Why would you let your kids watch this?!";
