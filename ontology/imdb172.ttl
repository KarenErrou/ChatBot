<#tt0056436-santoshini908>
	schema:about <#tt0056436>;
	schema:author "santoshini908";
	schema:dateCreated "Mon Jun 21 2010 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 8;
	schema:headline " Truly a movie for Future Generations of India";
