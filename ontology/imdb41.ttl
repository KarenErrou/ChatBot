<#tt0066612-ma_cortes>
	schema:about <#tt0066612>;
	schema:author "ma_cortes";
	schema:dateCreated "Sat Jan 23 2010 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 9;
	schema:headline " Good Zapata Spaghetti Western with communist revolutionaries and mean bandits";
