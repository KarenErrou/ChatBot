<#tt0092783-Doom>
	schema:about <#tt0092783>;
	schema:author "Doom";
	schema:dateCreated "Fri Dec 18 1998 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 10;
	schema:headline " Pretty good actionthriller... KILLER ending!";
