	schema:about <#tt0411269>;
	schema:author "Spoonhui";
	schema:dateCreated "Sun Jan 10 2010 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 1;
	schema:headline " Simply rubbish";
