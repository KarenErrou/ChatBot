	schema:about <#tt0066763>;
	schema:author "nrajesh";
	schema:dateCreated "Wed Apr 26 2000 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 9;
	schema:headline " True Classic";
