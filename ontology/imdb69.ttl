<#tt0129023-LenorabelLeFey>
	schema:about <#tt0129023>;
	schema:author "LenorabelLeFey";
	schema:dateCreated "Sat Feb 14 2004 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 7;
	schema:headline " What a great movie!";
