<#tt0264395-brian_vita>
	schema:about <#tt0264395>;
	schema:author "brian_vita";
	schema:dateCreated "Fri Mar 28 2003 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 1;
	schema:headline " A real stinker";
