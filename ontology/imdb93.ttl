<#tt0090297-Eric_157>
	schema:about <#tt0090297>;
	schema:author "Eric_157";
	schema:dateCreated "Tue Jun 08 1999 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 8;
	schema:headline " A precursor of the New British Cinema";
