<#tt0120783-Adriane>
	schema:about <#tt0120783>;
	schema:author "Adriane";
	schema:dateCreated "Thu Jan 21 1999 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 10;
	schema:headline " Cute remake of a great original";
