<#tt0118641-Sam_285>
	schema:about <#tt0118641>;
	schema:author "Sam_285";
	schema:dateCreated "Sun Aug 03 2003 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 7;
	schema:headline " Very interesting entertaining and educational";
