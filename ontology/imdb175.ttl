<#tt0114863-juan_hb>
	schema:about <#tt0114863>;
	schema:author "juan_hb";
	schema:dateCreated "Sat Mar 15 2003 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:headline " SHEER RUBBISH";
