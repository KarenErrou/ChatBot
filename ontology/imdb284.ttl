	schema:about <#tt0058620>;
	schema:author "ChiefGoreMongral";
	schema:dateCreated "Mon Aug 28 2006 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 10;
	schema:headline " Gore Mongral Movie Review StraitJacket";
