<#tt0055277-Steve_193>
	schema:about <#tt0055277>;
	schema:author "Steve_193";
	schema:dateCreated "Mon Sep 28 1998 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 9;
	schema:headline " EXCELLENT family movie. Fun and lighthearted.";
