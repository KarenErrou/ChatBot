<#tt0210357-johnsairships>
	schema:about <#tt0210357>;
	schema:author "johnsairships";
	schema:dateCreated "Sat Apr 12 2003 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 9;
