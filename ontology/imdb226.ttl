<#tt0094792-Rottweiler>
	schema:about <#tt0094792>;
	schema:author "Rottweiler";
	schema:dateCreated "Thu Jun 27 2002 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 7;
	schema:headline " Really good!! not if you are a leftwing";
