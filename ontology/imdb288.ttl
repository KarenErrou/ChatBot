<#tt0122933-Boyo_2>
	schema:about <#tt0122933>;
	schema:author "Boyo_2";
	schema:dateCreated "Mon Mar 29 1999 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 8;
	schema:headline " Extra funny";
