<#tt0042276-stephenpaultaylor>
	schema:about <#tt0042276>;
	schema:author "stephenpaultaylor";
	schema:dateCreated "Thu Sep 18 2003 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 7;
	schema:headline " oh the great US of A";
