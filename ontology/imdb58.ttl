<#tt0209906>
	rdf:type schema:Movie;
	schema:identifier "tt0209906";
	schema:name "Alla ricerca di Tadzio (TV Short 1970)";
	schema:duration "30min";
	schema:dateCreated "1970";
	schema:text "    Documentary on Luchino Visconti's travels and auditions in search for a young actor to portray the role of Tadzio in his adaptation of Thomas Mann's novella Death in Venice finally choosing Swedish actor Bjrn Andrsen.";
	schema:aggregateRating <#tt0209906-aggregateRating>;
	schema:actor "Luchino Visconti";
	schema:actor "Bjrn Andrsen";
	schema:character "Himself";
	schema:character "Himself";
