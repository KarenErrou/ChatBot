<#tt0089079-mikki_dr09>
	schema:about <#tt0089079>;
	schema:author "mikki_dr09";
	schema:dateCreated "Sat Feb 10 2007 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 9;
	schema:headline " Eleny a great movie to learned.";
