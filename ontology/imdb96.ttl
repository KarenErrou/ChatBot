<#tt0120780-daveisit>
	schema:about <#tt0120780>;
	schema:author "daveisit";
	schema:dateCreated "Sat Dec 23 2000 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 1;
	schema:headline " An Awful Awful Display";
