<#tt0104201-dwpollar>
	schema:about <#tt0104201>;
	schema:author "dwpollar";
	schema:dateCreated "Sun Mar 25 2001 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 7;
	schema:headline " The film leaves too many questions unanswered...";
