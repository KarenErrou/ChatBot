	schema:author "cynkat";
	schema:dateCreated "Thu Sep 24 2009 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 1;
	schema:headline " Everyone connected to this movie should be slapped repeatedly by the ghost of Henry James";
