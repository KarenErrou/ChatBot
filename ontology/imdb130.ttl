<#tt0300471-ornshaw>
	schema:about <#tt0300471>;
	schema:author "ornshaw";
	schema:dateCreated "Sun Feb 16 2003 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 10;
	schema:headline " Expected nothing but LOVED IT!";
