<#tt0064158-elo_equipamentos>
	schema:about <#tt0064158>;
	schema:author "elo_equipamentos";
	schema:dateCreated "Thu Jan 18 2018 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 2;
