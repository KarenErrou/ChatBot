<#tt0111732-nefar>
	schema:about <#tt0111732>;
	schema:author "nefar";
	schema:dateCreated "Sun Sep 29 2002 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 9;
	schema:headline " amazing";
