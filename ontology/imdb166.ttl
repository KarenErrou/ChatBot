<#tt0172184-NemkeSRB>
	schema:about <#tt0172184>;
	schema:author "NemkeSRB";
	schema:dateCreated "Sat Apr 20 2013 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:headline " One of the worst movies I have ever seen!";
