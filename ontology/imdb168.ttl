<#tt0081184-riker1985>
	schema:about <#tt0081184>;
	schema:author "riker1985";
	schema:dateCreated "Fri Aug 15 2003 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 7;
	schema:headline " Worst movie......ever!";
