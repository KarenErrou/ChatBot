<#tt0057556-catchclaw>
	schema:about <#tt0057556>;
	schema:author "catchclaw";
	schema:dateCreated "Thu Sep 12 2002 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 3;
	schema:headline " Loved it!";
