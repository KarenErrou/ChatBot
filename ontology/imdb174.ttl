<#tt0060726-dweilermg_1>
	schema:about <#tt0060726>;
	schema:author "dweilermg_1";
	schema:dateCreated "Fri Jul 27 2018 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 7;
	schema:headline "  Color Wrong!";
