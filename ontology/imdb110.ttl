	schema:about <#tt0086465>;
	schema:author "mab8485";
	schema:dateCreated "Fri Sep 12 2003 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 10;
	schema:headline " Very funny very entertaining movie  murphy's best?";
