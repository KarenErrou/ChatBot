<#tt1024942-nielsfynbo>
	schema:about <#tt1024942>;
	schema:author "nielsfynbo";
	schema:dateCreated "Sun Sep 23 2007 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 3;
	schema:headline " Sucks!";
