<#tt0464196-ruslabon_99>
	schema:about <#tt0464196>;
	schema:author "ruslabon_99";
	schema:dateCreated "Thu Sep 28 2006 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 1;
	schema:headline " One of the worst films i've ever had the displeasure of watching!!!";
