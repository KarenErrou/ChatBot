	schema:about <#tt0075019>;
	schema:author "Grand_Moff_Monkey";
	schema:dateCreated "Fri Apr 19 2002 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 9;
	schema:headline " Most underrated war film of all time!";
