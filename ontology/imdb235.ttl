<#tt0086998-hfennemore>
	schema:about <#tt0086998>;
	schema:author "hfennemore";
	schema:dateCreated "Fri Aug 25 2006 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 10;
	schema:headline " Takes you back and keeps you young!";
