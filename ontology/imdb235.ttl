<#tt0478209-BlindPK>
	schema:about <#tt0478209>;
	schema:author "BlindPK";
	schema:dateCreated "Sat Jul 08 2006 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 10;
	schema:headline " The Best Documentary ever!!!";
