<#tt0145893-ms_determined>
	schema:about <#tt0145893>;
	schema:author "ms_determined";
	schema:dateCreated "Thu Apr 30 2009 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 9;
	schema:headline " simply irresistible...";
