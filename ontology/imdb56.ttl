<#tt0093713-haifengg>
	schema:about <#tt0093713>;
	schema:author "haifengg";
	schema:dateCreated "Fri Aug 11 2000 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 10;
	schema:headline " A touching masterpiece much better than most Hollywood movies...";
