<#tt2180477-kailashsankar>
	schema:about <#tt2180477>;
	schema:author "kailashsankar";
	schema:dateCreated "Sun Jan 15 2012 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 10;
	schema:headline " A feel good movie with a perfect star cast";
