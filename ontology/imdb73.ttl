	schema:about <#tt0120791>;
	schema:author "dwyer";
	schema:dateCreated "Sun Dec 26 1999 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 10;
	schema:headline " Way better than the score would indicate";
