<#tt0127626-andrew_133>
	schema:about <#tt0127626>;
	schema:author "andrew_133";
	schema:dateCreated "Mon Nov 15 1999 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 7;
	schema:headline " An excellent spoof of gangster movies.";
