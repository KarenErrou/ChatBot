<#tt0088083-javah>
	schema:about <#tt0088083>;
	schema:author "javah";
	schema:dateCreated "Wed Jul 11 2001 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 10;
	schema:headline " No doubt the best Polish SciFi movie...";
