	schema:about <#tt0097174>;
	schema:author "itshack";
	schema:dateCreated "Sun Nov 04 2001 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 8;
	schema:headline " HAHAHAHAHAHAHA!!";
