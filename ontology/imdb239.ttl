<#tt0494318-BA_Harrison>
	schema:about <#tt0494318>;
	schema:author "BA_Harrison";
	schema:dateCreated "Thu Dec 18 2014 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 3;
	schema:headline " A horrorfan's review.";
