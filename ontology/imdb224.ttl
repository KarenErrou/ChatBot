<#tt0101492-mhorg2018>
	schema:about <#tt0101492>;
	schema:author "mhorg2018";
	schema:dateCreated "Mon Jul 02 2018 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 7;
	schema:headline " Lame";
