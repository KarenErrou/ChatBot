	schema:about <#tt0067017>;
	schema:author "Blitz_5";
	schema:dateCreated "Wed Jan 13 1999 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:headline " If I find it on 16mm I'll show it at my drivein!";
