<#tt0208003-garber_1>
	schema:about <#tt0208003>;
	schema:author "garber_1";
	schema:dateCreated "Sun Jan 25 2004 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 10;
	schema:headline " On my top ten comedy list";
