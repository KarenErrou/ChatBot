<#tt0086393-KentBrockman>
	schema:about <#tt0086393>;
	schema:author "KentBrockman";
	schema:dateCreated "Tue Sep 23 2003 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 10;
	schema:headline " Fun Cheesy humor";
