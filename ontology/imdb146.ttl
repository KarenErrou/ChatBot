<#tt0240033-archyon>
	schema:about <#tt0240033>;
	schema:author "archyon";
	schema:dateCreated "Sun Dec 31 2000 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 8;
	schema:headline " ABSOLUTE GARBAGE!!!";
