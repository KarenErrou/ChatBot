<#tt0072097>
	rdf:type schema:Movie;
	schema:identifier "tt0072097";
	schema:name "Romanzo popolare (1974)";
	schema:duration "1h46min";
	schema:dateCreated "1974";
	schema:text "    50 year old Giulio Tognazzi and his 17 year old goddaughter Vincenzina Muti fall madly in love with each other and soon are wed. Giulio's jealousy gets in the way of their happiness though. Has Vincenzina cheated on him with his younger friend or not?";
	schema:aggregateRating <#tt0072097-aggregateRating>;
