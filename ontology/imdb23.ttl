	schema:about <#tt1781069>;
	schema:author "jazzysaravana";
	schema:dateCreated "Wed Jan 26 2011 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 10;
	schema:headline " A Priceless jewel which cannot be comprehended it must be witnessed!";
