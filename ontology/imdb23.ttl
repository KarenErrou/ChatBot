<#tt0798817-CarsonTrent>
	schema:about <#tt0798817>;
	schema:author "CarsonTrent";
	schema:dateCreated "Mon May 09 2011 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 1;
	schema:headline " Perfect Zero";
