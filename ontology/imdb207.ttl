<#tt0074473-hippiedj>
	schema:about <#tt0074473>;
	schema:author "hippiedj";
	schema:dateCreated "Tue May 18 2004 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 3;
	schema:headline " Hooray for 1970's hairy pseudohardcore!";
