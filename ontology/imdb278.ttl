<#tt0345043-khiem_lu>
	schema:about <#tt0345043>;
	schema:author "khiem_lu";
	schema:dateCreated "Sat Feb 02 2008 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 7;
	schema:headline " Scrappy movie!!!!";
