<#tt0093854-jcmcm>
	schema:about <#tt0093854>;
	schema:author "jcmcm";
	schema:dateCreated "Mon Dec 13 1999 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 7;
	schema:headline " Not that bad.";
