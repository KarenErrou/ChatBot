<#tt0071269-yohav23>
	schema:about <#tt0071269>;
	schema:author "yohav23";
	schema:dateCreated "Sat Jul 05 2003 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:headline " an underestimated gem";
