	schema:about <#tt0246464>;
	schema:author "yakkorat";
	schema:dateCreated "Tue Oct 29 2002 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 10;
	schema:headline " One of the Funniest Movies I've Seen in a Year";
