<#tt0291502-MarieGabrielle>
	schema:about <#tt0291502>;
	schema:author "MarieGabrielle";
	schema:dateCreated "Wed Sep 20 2006 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 1;
	schema:headline " Rent the Lina Wertmuller original...";
