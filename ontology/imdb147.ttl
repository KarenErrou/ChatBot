	schema:author "rooprect";
	schema:dateCreated "Wed Feb 07 2007 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 2;
	schema:headline " Not good enough to be art. Not bad enough to be porn.";
