<#tt0077942-moonspinner55>
	schema:about <#tt0077942>;
	schema:author "moonspinner55";
	schema:dateCreated "Wed Mar 12 2003 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 1;
	schema:headline " Get out your magnifying glass and you can catch a few glimpses of truth in the screenplay";
