<#tt1646959-Greenstar99>
	schema:about <#tt1646959>;
	schema:author "Greenstar99";
	schema:dateCreated "Wed Oct 07 2015 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 9;
	schema:headline " Revenge with a kill or get killed attitude with totally craziness";
