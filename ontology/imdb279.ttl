<#tt0255094-dhandley>
	schema:about <#tt0255094>;
	schema:author "dhandley";
	schema:dateCreated "Thu Nov 22 2001 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:headline " Do not see this movie if you are already depressed!";
