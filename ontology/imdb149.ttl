<#tt0090305-abhilash7>
	schema:about <#tt0090305>;
	schema:author "abhilash7";
	schema:dateCreated "Tue Dec 23 2003 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 2;
	schema:headline " Really Good";
