<#tt0052533-J__Steed>
	schema:about <#tt0052533>;
	schema:author "J__Steed";
	schema:dateCreated "Sun Jul 11 1999 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 4;
	schema:headline " JUST NOT BAD ENOUGH AIRPORTLIKE TRASH";
