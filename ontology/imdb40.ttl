<#tt0060200-togan>
	schema:about <#tt0060200>;
	schema:author "togan";
	schema:dateCreated "Tue Apr 15 2003 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 5;
	schema:headline " Wonderful film with absolute sense of humor.";
