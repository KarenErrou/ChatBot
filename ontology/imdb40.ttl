<#tt0034175-whpratt1>
	schema:about <#tt0034175>;
	schema:author "whpratt1";
	schema:dateCreated "Sat Jul 05 2003 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 8;
	schema:headline " A CAST OF CLASSIC GIANTS OF THE SILVER SCREEN!";
