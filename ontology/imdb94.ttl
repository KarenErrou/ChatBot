<#tt0094077-Michael_Pilkington>
	schema:about <#tt0094077>;
	schema:author "Michael_Pilkington";
	schema:dateCreated "Fri Apr 16 1999 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 7;
	schema:headline " A real disappointment from Troma";
