<#tt0107525-alucifer>
	schema:about <#tt0107525>;
	schema:author "alucifer";
	schema:dateCreated "Sat Sep 10 2005 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 5;
	schema:headline " lots of martial arts action";
