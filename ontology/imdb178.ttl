<#tt0332379-teleadm_persson>
	schema:about <#tt0332379>;
	schema:author "teleadm_persson";
	schema:dateCreated "Fri Oct 07 2005 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 10;
	schema:headline " This must be one of the best feel good movies ever!";
