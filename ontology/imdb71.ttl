<#tt0145660-KEITH_LANCASTER>
	schema:about <#tt0145660>;
	schema:author "KEITH_LANCASTER";
	schema:dateCreated "Tue Jun 25 2002 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 1;
	schema:headline " If this trash is funny then so is a severe dose of dysentery.";
