<#tt0079100-schempp>
	schema:about <#tt0079100>;
	schema:author "schempp";
	schema:dateCreated "Sun May 05 2002 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 9;
	schema:headline " subtle deep perfect";
