<#tt0250797-DunnDeeDaGreat>
	schema:about <#tt0250797>;
	schema:author "DunnDeeDaGreat";
	schema:dateCreated "Sat May 18 2002 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 10;
	schema:headline " A summer blockbuster for adults";
