<#tt0065642-helpless_dancer>
	schema:about <#tt0065642>;
	schema:author "helpless_dancer";
	schema:dateCreated "Fri Mar 31 2000 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 7;
	schema:headline " What a dirty low down snake!";
