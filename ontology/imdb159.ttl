<#tt1586265-werefox08>
	schema:about <#tt1586265>;
	schema:author "werefox08";
	schema:dateCreated "Wed May 30 2012 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 3;
	schema:headline " I Am An Optimist.....I Expected A Movie";
