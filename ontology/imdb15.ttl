<#tt0473188-eonianskr>
	schema:about <#tt0473188>;
	schema:author "eonianskr";
	schema:dateCreated "Fri Jan 19 2007 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 2;
	schema:headline " Macho man Paul Walker strikes again";
