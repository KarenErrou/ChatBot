	schema:about <#tt0395669>;
	schema:author "WakenPayne";
	schema:dateCreated "Mon Jul 04 2011 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 1;
	schema:headline " How?";
