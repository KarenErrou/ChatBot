<#tt0086972-buchass>
	schema:about <#tt0086972>;
	schema:author "buchass";
	schema:dateCreated "Mon Apr 26 2010 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:headline " The zero degree of Sword and Sorcery Flicks...";
