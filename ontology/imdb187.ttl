<#tt0060884-kenandraf>
	schema:about <#tt0060884>;
	schema:author "kenandraf";
	schema:dateCreated "Tue Sep 25 2001 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 4;
