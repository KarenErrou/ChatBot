<#tt0216216-ma_cortes>
	schema:about <#tt0216216>;
	schema:author "ma_cortes";
	schema:dateCreated "Mon Dec 15 2008 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 7;
	schema:headline " A clichd scifi thriller involved in humor touches by Arnold Schwarzenegger";
