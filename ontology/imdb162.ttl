<#tt0280486-scott_logan_197_874075>
	schema:about <#tt0280486>;
	schema:author "scott_logan_197_874075";
	schema:dateCreated "Thu Aug 23 2018 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:headline " Surprisingly Good Movie worth a watch";
