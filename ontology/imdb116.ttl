<#tt1301303-fraserpatty>
	schema:about <#tt1301303>;
	schema:author "fraserpatty";
	schema:dateCreated "Tue Dec 01 2009 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 7;
	schema:headline " This is an awesome movie!";
