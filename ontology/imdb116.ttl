<#tt0116275-alli_katz>
	schema:about <#tt0116275>;
	schema:author "alli_katz";
	schema:dateCreated "Sun Jun 03 2001 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 8;
	schema:headline " Poignant intimate movie";
