<#tt1032815-day_andy>
	schema:about <#tt1032815>;
	schema:author "day_andy";
	schema:dateCreated "Sun Sep 27 2009 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 10;
	schema:headline " Negative reviews are wrong";
