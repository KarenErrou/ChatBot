<#tt0264150-kursietis>
	schema:about <#tt0264150>;
	schema:author "kursietis";
	schema:dateCreated "Wed Aug 20 2003 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:headline " one of the worst movies I have ever seen...";
