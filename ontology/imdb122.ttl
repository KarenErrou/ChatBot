<#tt0092962-MaRX_4>
	schema:about <#tt0092962>;
	schema:author "MaRX_4";
	schema:dateCreated "Wed Jan 27 1999 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:headline " Could have been better.";
