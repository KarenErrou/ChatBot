<#tt0098238-kickright>
	schema:about <#tt0098238>;
	schema:author "kickright";
	schema:dateCreated "Fri Dec 05 2003 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 8;
	schema:headline " A great History lesson!";
