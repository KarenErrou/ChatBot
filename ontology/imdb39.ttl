<#tt0348150-nocritiqueplease>
	schema:about <#tt0348150>;
	schema:author "nocritiqueplease";
	schema:dateCreated "Thu Jul 06 2006 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 1;
	schema:headline " SuperDepri makes me sick.";
