<#tt0120701-flexico>
	schema:about <#tt0120701>;
	schema:author "flexico";
	schema:dateCreated "Sat Oct 07 2000 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 10;
	schema:headline " I love this movie!";
