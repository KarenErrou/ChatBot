<#tt0220506-tripperM>
	schema:about <#tt0220506>;
	schema:author "tripperM";
	schema:dateCreated "Thu Apr 24 2003 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:headline " what the heck?!...";
