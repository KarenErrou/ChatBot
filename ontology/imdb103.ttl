<#tt0077928-caespoo>
	schema:about <#tt0077928>;
	schema:author "caespoo";
	schema:dateCreated "Sat Sep 08 2007 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 1;
	schema:headline " Idiotic and blatantly racist";
