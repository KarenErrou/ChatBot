<#tt0143127-kyle_mcdonald>
	schema:about <#tt0143127>;
	schema:author "kyle_mcdonald";
	schema:dateCreated "Fri Jan 04 2008 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 8;
	schema:headline " Good Batman Cartoon.";
