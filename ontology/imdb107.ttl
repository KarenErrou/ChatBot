<#tt1145144-korina_cruz>
	schema:about <#tt1145144>;
	schema:author "korina_cruz";
	schema:dateCreated "Tue Nov 25 2008 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 10;
	schema:headline " I really loved the movie";
