<#tt0293815-giatime>
	schema:about <#tt0293815>;
	schema:author "giatime";
	schema:dateCreated "Sat Oct 11 2003 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 9;
	schema:headline " I almost dropped dead laughing!";
