<#tt0044462-dhaufrect>
	schema:about <#tt0044462>;
	schema:author "dhaufrect";
	schema:dateCreated "Tue Dec 10 2002 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 2;
	schema:headline " 3 D Excitement";
