<#tt0164334-jeffhrsn>
	schema:about <#tt0164334>;
	schema:author "jeffhrsn";
	schema:dateCreated "Wed Apr 25 2001 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:headline " This film is just AWFUL!";
