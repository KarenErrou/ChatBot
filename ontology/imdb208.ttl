<#tt0068284-HumanoidOfFlesh>
	schema:about <#tt0068284>;
	schema:author "HumanoidOfFlesh";
	schema:dateCreated "Sun Apr 30 2006 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 7;
	schema:headline " Groovy blacksploitation classic.";
