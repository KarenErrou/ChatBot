	schema:author "eade";
	schema:dateCreated "Thu Nov 08 2001 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 10;
	schema:headline " A man accidentally fakes his own suicide and tries to live a new carefree life.";
