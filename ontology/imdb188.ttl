<#tt0078212-Ilde>
	schema:about <#tt0078212>;
	schema:author "Ilde";
	schema:dateCreated "Wed Aug 20 2003 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 8;
	schema:headline " I loved it and I'm proud to be Swiss";
