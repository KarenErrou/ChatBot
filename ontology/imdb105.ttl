<#tt0430919-perkop>
	schema:about <#tt0430919>;
	schema:author "perkop";
	schema:dateCreated "Fri Aug 31 2007 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 1;
	schema:headline " Beware complete crap";
