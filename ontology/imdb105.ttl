	schema:about <#tt0119522>;
	schema:author "nikkismith_312";
	schema:dateCreated "Fri Jan 26 2001 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 7;
	schema:headline " Quite good actually!";
