<#tt0271259-sapagap>
	schema:about <#tt0271259>;
	schema:author "sapagap";
	schema:dateCreated "Mon Dec 17 2007 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:headline " Do someone knows...?";
