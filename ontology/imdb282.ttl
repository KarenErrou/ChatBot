<#tt0070915-turkish2>
	schema:about <#tt0070915>;
	schema:author "turkish2";
	schema:dateCreated "Mon Mar 10 2003 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 10;
	schema:headline " very cool film";
