	schema:about <#tt2002718>;
	schema:author "Filmaniac123";
	schema:dateCreated "Wed Dec 04 2013 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 2;
	schema:headline " Really 6 out of 10??!!";
