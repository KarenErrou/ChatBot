<#tt0292542-juanfoucault>
	schema:about <#tt0292542>;
	schema:author "juanfoucault";
	schema:dateCreated "Thu Nov 27 2003 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 9;
	schema:headline " Best Romantic movie of all times";
