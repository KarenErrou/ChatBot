<#tt0059742-michyh>
	schema:about <#tt0059742>;
	schema:author "michyh";
	schema:dateCreated "Thu May 04 2000 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 10;
	schema:headline " Timeless Classic";
