<#tt0109627-StlBlade>
	schema:about <#tt0109627>;
	schema:author "StlBlade";
	schema:dateCreated "Thu Jul 22 1999 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 10;
	schema:headline " Yes one of 'those' movies.";
