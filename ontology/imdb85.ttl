<#tt0083564-Snoopy1>
	schema:about <#tt0083564>;
	schema:author "Snoopy1";
	schema:dateCreated "Sun Jan 05 2003 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 10;
	schema:headline " I still love it!";
