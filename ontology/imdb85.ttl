<#tt0161981-yuuske007>
	schema:about <#tt0161981>;
	schema:author "yuuske007";
	schema:dateCreated "Wed Jun 27 2001 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:headline " Series was better but an awesome movie!";
