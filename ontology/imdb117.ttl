<#tt0063612-kreeper>
	schema:about <#tt0063612>;
	schema:author "kreeper";
	schema:dateCreated "Wed Jul 31 2002 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:headline " unredeemable excrement";
