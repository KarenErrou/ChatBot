<#tt0438315-ma_cortes>
	schema:about <#tt0438315>;
	schema:author "ma_cortes";
	schema:dateCreated "Tue Jul 10 2012 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 6;
	schema:headline " Interesting and strange story about a gymnast inspired by true events";
