<#tt0093437-wmac>
	schema:about <#tt0093437>;
	schema:author "wmac";
	schema:dateCreated "Mon Oct 04 1999 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:headline " I don't get it?";
