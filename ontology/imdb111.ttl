<#tt0233469-otoka>
	schema:about <#tt0233469>;
	schema:author "otoka";
	schema:dateCreated "Tue Aug 13 2002 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 5;
	schema:headline " Go rent Commando instead  110";
