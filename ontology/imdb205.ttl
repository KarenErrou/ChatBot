<#tt0054615-Emmjewels>
	schema:about <#tt0054615>;
	schema:author "Emmjewels";
	schema:dateCreated "Fri Jan 03 2003 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 10;
	schema:headline " 40 years and still worth seeing.";
