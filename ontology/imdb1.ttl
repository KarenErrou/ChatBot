<#tt0366518-Fed7haS>
	schema:about <#tt0366518>;
	schema:author "Fed7haS";
	schema:dateCreated "Thu Feb 10 2005 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 7;
	schema:headline " Interesting movie... and a surprise performance!!";
