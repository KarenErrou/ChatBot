<#tt0436992-bittenapplesss>
	schema:about <#tt0436992>;
	schema:author "bittenapplesss";
	schema:dateCreated "Mon Oct 22 2018 00:00:00 GMT+0200 (Central European Summer Time)";
	schema:reviewRating 1;
	schema:headline " Where's the doctor ?";
