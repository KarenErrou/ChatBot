<#tt0827521-arrayat>
	schema:about <#tt0827521>;
	schema:author "arrayat";
	schema:dateCreated "Fri Feb 29 2008 00:00:00 GMT+0100 (Central European Standard Time)";
	schema:reviewRating 8;
	schema:headline " It's so far from BAD.. it's the worst.";
